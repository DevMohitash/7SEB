#!/system/bin/sh
#=======================================#
#VARIABLES===============================#
BB=/data/adb/magisk/busybox
BRANCH="4-Revert_To_Stock"
#=======================================#
#=======================================#

sleep 2

$BB mkdir /data/media/0/mkadpse

$BB rm -rf /data/vendor/dolby/dax_sqlite3.db

$BB rm -rf /data/data/com.oneplus.sound.tuner/databases/sound_tuner.db

$BB wget https://raw.githubusercontent.com/DevMohitash/7SEB/1-Normal_Enhancement_Boost/mixer/stock/mixer_paths_tavil.xml -P /data/media/0/mkadpse
$BB cp -rf /data/media/0/mkadpse/mixer_paths_tavil.xml /vendor/etc/mixer_paths_tavil.xml

$BB rm -rf /data/media/0/mkadpse

$BB rm -rf /data/media/0/YourSEchanges.log

sleep 5
reboot
